{
  "name": "TaxiApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "assets": "npx react-native-asset",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "expo-fix": "npx expo install --fix",
    "eas-login": "eas login",
    "eas-config": "yarn expo-fix && eas build:configure",
    "eas-android": " yarn expo-fix && eas build --platform android",
    "eas-ios": " yarn expo-fix && eas build --platform ios",
    "eas-all": " yarn expo-fix && eas build --platform all",
    "eas-list": " yarn expo-fix && eas build:list",
    "android:dev": "react-native run-android --mode=developmentdebug --appId=com.taxi.platform.development",
    "android:stage": "react-native run-android --mode=stagingrelease --appId=com.taxi.platform.staging",
    "android:prod": "react-native run-android --mode=productionrelease --appId=com.taxi.platform"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.3.0",
    "@react-native-google-signin/google-signin": "^12.2.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "add": "^2.0.6",
    "expo": "51",
    "expo-apple-authentication": "~6.4.2",
    "expo-checkbox": "~3.0.0",
    "expo-dev-client": "~4.0.25",
    "expo-font": "~12.0.9",
    "expo-image": "~1.12.13",
    "expo-splash-screen": "~0.27.5",
    "expo-tracking-transparency": "~4.0.2",
    "react": "18.2.0",
    "react-hook-form": "^7.52.2",
    "react-native": "0.74.5",
    "react-native-config": "^1.5.3",
    "react-native-fbsdk-next": "^13.0.0",
    "react-native-onboarding-swiper": "^1.3.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.87",
    "@react-native/eslint-config": "0.74.87",
    "@react-native/metro-config": "0.74.87",
    "@react-native/typescript-config": "0.74.87",
    "@types/react": "~18.2.79",
    "@types/react-native-onboarding-swiper": "^1.1.9",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
